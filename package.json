{
  "name": "@bmjs/obs-utils",
  "type": "module",
  "version": "1.0.3",
  "description": "obs-utils",
  "author": "ywenhao",
  "license": "MIT",
  "keywords": [
    "obs-utils",
    "huawei obs-utils"
  ],
  "sideEffects": false,
  "exports": {
    "./vite": {
      "import": {
        "types": "./dist/vite/index.d.ts",
        "default": "./dist/vite/index.js"
      },
      "require": {
        "types": "./dist/vite/index.d.cts",
        "default": "./dist/vite/index.cjs"
      }
    },
    "./upload": {
      "import": {
        "types": "./dist/upload/index.d.ts",
        "default": "./dist/upload/index.js"
      },
      "require": {
        "types": "./dist/upload/index.d.cts",
        "default": "./dist/upload/index.cjs"
      }
    },
    "./*": "./*"
  },
  "bin": {
    "obs-upload": "./dist/upload/index.js"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch src",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "dependencies": {
    "ansis": "^4.1.0",
    "consola": "^3.4.2",
    "esdk-obs-nodejs": "^3.25.6",
    "fast-glob": "^3.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "@antfu/ni": "^25.0.0",
    "@types/node": "^24.2.0",
    "tsdown": "^0.13.3",
    "unconfig": "^7.3.2"
  }
}
