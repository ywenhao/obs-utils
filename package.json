{
  "name": "@bmjs/obs-utils",
  "type": "module",
  "version": "1.0.8",
  "description": "obs-utils",
  "author": "ywenhao",
  "license": "MIT",
  "keywords": [
    "obs-utils",
    "huawei obs-utils"
  ],
  "sideEffects": false,
  "exports": {
    "./vite": {
      "import": {
        "types": "./dist/vite/index.d.mts",
        "default": "./dist/vite/index.mjs"
      },
      "require": {
        "types": "./dist/vite/index.d.cts",
        "default": "./dist/vite/index.cjs"
      }
    },
    "./upload": {
      "import": {
        "types": "./dist/upload/index.d.mts",
        "default": "./dist/upload/index.mjs"
      },
      "require": {
        "types": "./dist/upload/index.d.cts",
        "default": "./dist/upload/index.cjs"
      }
    },
    "./*": "./*"
  },
  "bin": {
    "obs-upload": "./dist/upload/index.mjs"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "dependencies": {
    "ansis": "^4.2.0",
    "consola": "^3.4.2",
    "esdk-obs-nodejs": "^3.25.6",
    "fast-glob": "^3.3.3",
    "jiti": "^2.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^25.2.3",
    "eslint": "^9.39.2",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "unconfig": "^7.4.2"
  }
}
